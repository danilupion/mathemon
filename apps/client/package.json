    "nes.css": "^2.3.0",
    "pokemon-sprites": "github:PokeAPI/sprites",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.1",
    "react-scripts": "5.0.1",
    "sass": "^1.57.1",
