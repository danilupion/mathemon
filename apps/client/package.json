    "prettier": "^2.8.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.js": [
      "yarn prettify"
    ],
    "src/**/*.{ts,tsx}": [
      "yarn prettify",
      "yarn lint:fix --max-warnings=0",
      "yarn test"
    ]
  }
